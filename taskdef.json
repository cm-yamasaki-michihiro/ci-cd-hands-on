{
  "executionRoleArn": "arn:aws:iam::738965884675:role/fugafuga-ECSTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fizzbuzz",
      "image": "<IMAGE>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "fugafuga-Task"
}
